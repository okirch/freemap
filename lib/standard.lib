discovery-scan default {
	broadcast-probe icmp {
	}
}

host-scan default {
	host-probe arp {
		optional true;
	}
	host-probe icmp {
		ttl	128;
	}
}

host-scan thorough {
	host-probe arp {
		optional true;
	}
	host-probe icmp {
		ttl	128;
	};

	# This probe tries to elicit a TCP RST response from the target host.
	# Note, this probe doesn't do anything useful if the host you run
	# on has firewalling enabled (the response will be discarded).
	host-probe tcp {
		tcp-flags syn, ack;
		tcp-dst-port 80;
	}
}

host-scan experimental {
	port-probe ipproto {
		optional true;
		protocol 6, 33;
	}
}

topology-scan traceroute {
	topo-probe traceroute {
		packet-proto udp;
	}
}

topology-scan traceroute-tcp {
	topo-probe traceroute {
		packet-proto tcp;
	}
}

port-scan minimal {
	port-probe tcp {
		port 22, 25, 80;
	}
	port-probe udp {
		port 111;
	}
}

port-scan default {
	port-probe tcp {
		port 1-20, 21, 22, 25, 53, 80, 137, 138, 139;
	}
	port-probe udp {
		port 53, 111, 2049, 5353, 137, 138, 139;
	}
}

port-scan thorough {
	port-probe tcp {
		port 1-512, 2049, 8080;
	}
	port-probe udp {
		port 1-512, 2049;
	}
}

service-catalog "default" {
	use	dns,
		"sunrpc.rpcbind",
		"sunrpc.nfsd";
}
