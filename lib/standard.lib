

host-scan default {
	host-probe arp {
		optional true;
	}
	host-probe icmp {
		ttl	128;
	}
}

host-scan thorough {
	host-probe arp {
		optional true;
	}
	host-probe icmp;
	port-probe tcp {
		port 0;
	}
}

host-scan experimental {
	port-probe ipproto {
		optional true;
		protocol 6, 33;
	}
}

topology-scan traceroute {
	topo-probe traceroute {
		packet-proto udp;
	}
}

port-scan minimal {
	port-probe tcp {
		port 22, 25, 80;
	}
	port-probe udp {
		port 111;
	}
}

port-scan default {
	port-probe tcp {
		port 1-20, 21, 22, 25, 53, 80, 137, 138, 139;
	}
	port-probe udp {
		port 53, 111, 2049, 5353, 137, 138, 139;
	}
}

port-scan thorough {
	port-probe tcp {
		port 1-512, 2049, 8080;
	}
	port-probe udp {
		port 1-512, 2049;
	}
}
