# This file describes service probes for DNS
service-probe "default" {
	tcp-ports	53;
	udp-ports	53;

	packet "dns-root-query" {
		payload
			0xca, 0xfe,	# ID
			0, 0,		# query, no flags
			0, 1,		# 1 query
			0, 0,		# 0 answers
			0, 0,		# 0 SOA records
			0, 0,		# 0 additional records

			# question string:
			0,		# 0-octet name component for root "."
			0, 1,		# qtype: A
			0, 1;		# qclass: IN
	}
}
